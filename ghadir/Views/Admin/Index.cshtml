@model IEnumerable<ghadir.Models.Admin>
@{
    ViewBag.Title = "Index";
}

@foreach (var item in Model)
{
    <form action="/Admin/Crud" method="post" style="margin-bottom: 50px">
        <div class="form-group">
            <label for="first_name">نام کاربری</label>
            <input type="text" class="form-control" id="first_name" name="UserName" value="@item.UserName">
        </div>
        <div class="form-group">
            <label for="last_name">رمز عبور</label>
            <input type="password" class="form-control" id="last_name" name="Password" value="@item.Password">
        </div>
        <button type="submit" class="btn btn-default">ورود</button>
    </form>
}
